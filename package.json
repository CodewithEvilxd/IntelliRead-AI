    "@neondatabase/serverless": "^1.0.2",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.7",
    "groq-sdk": "^0.29.0",
    "lucide-react": "^0.536.0",
    "mammoth": "^1.11.0",
    "pdfjs-dist": "^5.4.54",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.6",
    "terser": "^5.43.1",
    "youtube-transcript": "^1.2.1"
