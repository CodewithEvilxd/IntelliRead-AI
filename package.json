{
  "name": "snap-study",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Snap Study - AI-powered document analysis platform with vintage aesthetics",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
